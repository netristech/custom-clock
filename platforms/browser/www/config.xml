<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.custom-clock" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Custom Clock</name>
    <description>
        Make a custom schedule and display the current custom time.
    </description>
    <author email="info@netristech.com" href="https://www.netris.us">
        Netris Apps
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
